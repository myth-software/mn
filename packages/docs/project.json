{
  "name": "docs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/docs",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "packages/docs"
      },
      "outputs": ["packages/docs/build"]
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run serve",
        "cwd": "packages/docs"
      },
      "dependsOn": ["build"]
    }
  },
  "tags": []
}
